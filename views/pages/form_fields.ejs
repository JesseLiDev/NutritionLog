<div class="form-group">
    <label for="title">Title</label>
    <input required  value="<%= nutritionData.title%>" type="text" name="title" id="title" class="form-control"> 
</div>

<div class="form-group">
    <label for="description">Description</label>
    <textarea name="description"  id="description" class="form-control" ><%= nutritionData.description%></textarea> 
</div>

<div class="form-group">
    <label for="markdown">Markdown</label>
    <textarea required name="markdown" id="markdown" class="form-control" ><%= nutritionData.markdown%></textarea> 
</div>
</br>

<button type="submit" class="btn btn-primary"> Save </button>
<a href="/" class="btn btn-secondary">Cancel</a>
